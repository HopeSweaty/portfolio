    "@docusaurus/plugin-sitemap": "^3.5.2",
    "@docusaurus/preset-classic": "^3.5.2",
    "@docusaurus/theme-search-algolia": "^3.5.2",
    "@formspree/react": "^2.5.1",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
